{
  "name": "trading-saga",
  "version": "2.2.90",
  "private": true,
  "scripts": {
    "android-build-debug-win": "cd android && .\\gradlew assembleDebug",
    "android-build-release-win": "cd android && .\\gradlew assembleRelease",
    "android-build-debug": "cd android && ./gradlew assembleDebug",
    "android-build-release": "cd android && ./gradlew assembleRelease",
    "android-build-clean": "cd android && gradle clean --warning-mode all",
    "android-list-devices": "emulator -list-avds",
    "android-run": "react-native run-android --port=8081",
    "android-run-fold": "(emulator @8_Foldable_API_29 -accel on -no-snapshot-save & sleep 5) && npm run android-run",
    "android-accel": "emulator -accel-check",
    "ios-pod-install": "cd ios && pod install",
    "ios-list-targets": "xcodebuild -list -workspace ios/TradingSaga.xcworkspace",
    "ios-list-devices": "xcrun xctrace list devices",
    "ios-build-debug": "xcodebuild -workspace ios/TradingSaga.xcworkspace -scheme TradingSaga -configuration Debug build | xcpretty",
    "ios-build-release": "xcodebuild -workspace ios/TradingSaga.xcworkspace -scheme TradingSaga -configuration Release build | xcpretty",
    "ios-run": "react-native run-ios",
    "ios-run-iphone7": "npm run ios-run -- --simulator 'iPhone 7'",
    "ios-run-iphone11": "npm run ios-run -- --simulator 'iPhone 11 Pro Max'",
    "ios-run-ipadpro": "npm run ios-run -- --simulator 'iPad Pro (9.7-inch) (13.4)'",
    "postios-run-iphone7": "npm run metro-run",
    "postios-run-iphone11": "npm run metro-run",
    "postios-run-ipadpro": "npm run metro-run",
    "preios-build-debug": "npm run ios-pod-install",
    "preios-build-release": "npm run ios-pod-install",
    "metro-run": "npm run metro-stop & (react-native start --reset-cache)",
    "metro-stop": "kill -9 $(lsof -t -i:8081)",
    "premetro-run": "npm i",
    "postversion": "react-native-version",
    "patch": "npm version patch -m 'Upgrade to %s for reasons'",
    "version-ios": "RNV=ios react-native-version --target ios",
    "version-android": "RNV=android react-native-version --target android",
    "clean": "react-native clean-project",
    "android:clean": "cd android && .\\gradlew clean",
    "start": "npm run metro-run",
    "rename": "react-native-rename",
    "test": "jest --coverage",
    "lint": "eslint . --max-warnings=0",
    "lint:write": "eslint . --fix",
    "devtools": "react-devtools",
    "update-ios-version": ". ios/versioning.sh",
    "CI:PR:prebuild": "npm --unsafe-perm --no-git-tag-version version prerelease --preid=PR$PULL_REQUEST_ID-BLD$BITRISE_BUILD_NUMBER && npm version",
    "CI:MASTER:prebuild": "npm run publish -- --dry-run && npm run publish && npm run postversion && git push --follow-tags origin HEAD:master",
    "publish": "standard-version --message=\"chore: version update v%s [skip ci]\" --release-as patch",
    "storybook": "start-storybook -p 7007",
    "puml": "java -jar bin/plantuml.1.2020.15.jar -gui",
    "postinstall": "patch-package",
    "checkcommit": "commitlint --from=HEAD~1 -V",
    "prettify": "prettier --config .prettierrc.js 'app/**/*.{js,jsx,ts,tsx}' --write",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@amplitude/react-native": "^2.6.0",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/checkbox": "^0.3.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^6.0.0",
    "@react-native-community/toolbar-android": "0.1.0-rc.2",
    "@react-native-firebase/admob": "^11.5.0",
    "@react-native-firebase/analytics": "^11.5.0",
    "@react-native-firebase/app": "^11.5.0",
    "@react-native-firebase/crashlytics": "^11.5.0",
    "@react-navigation/native": "^5.2.3",
    "@react-navigation/stack": "^5.2.18",
    "@wowmaking/react-native-iron-source": "^7.0.4",
    "buffer": "^5.5.0",
    "color": "^3.1.3",
    "d3": "^5.15.0",
    "d3-scale": "^3.1.0",
    "fast-html-parser": "^1.0.1",
    "firebase": "^7.8.2",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^11.0.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "moment-duration-format": "^2.3.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-async": "^10.0.1",
    "react-countdown-hook": "^1.1.0",
    "react-native": "^0.62.2",
    "react-native-device-info": "^5.5.3",
    "react-native-elements": "^2.0.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-html-to-pdf": "^0.10.0",
    "react-native-iap": "5.1.1",
    "react-native-iaphub": "^6.2.5",
    "react-native-keep-awake": "^4.0.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-picker-select": "^7.0.0",
    "react-native-rate": "^1.2.9",
    "react-native-responsive-screen": "^1.4.0",
    "react-native-safe-area-context": "^3.3.0",
    "react-native-screens": "^2.7.0",
    "react-native-share": "^3.8.5",
    "react-native-size-matters": "^0.4.0",
    "react-native-slider": "^0.11.0",
    "react-native-sound": "^0.11.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^12.1.0",
    "react-native-svg-charts": "^5.4.0",
    "react-native-uuid": "latest",
    "react-native-vector-icons": "^6.6.0",
    "react-native-webview": "^11.2.1",
    "rxjs": "^6.5.5",
    "sbd": "^1.0.19",
    "url": "^0.11.0",
    "use-async-effect": "^2.2.3",
    "util": "^0.12.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-import-glob-simplified": "https://gitlab.com/fantasy-mesa/babel-plugin-import-glob-simplified.git#master",
    "@babel/preset-flow": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/runtime": "^7.9.6",
    "@bam.tech/react-native-make": "^3.0.3",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@react-native-community/eslint-config": "^1.1.0",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-ondevice-knobs": "^5.3.19",
    "@storybook/react-native": "^5.3.19",
    "@types/color": "^3.0.2",
    "@types/d3": "^5.7.2",
    "@types/fast-html-parser": "^1.0.2",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.172",
    "@types/moment-duration-format": "^2.2.3",
    "@types/react": "^16.9.34",
    "@types/react-native": "^0.62.7",
    "@types/react-native-html-to-pdf": "^0.8.1",
    "@types/react-native-keep-awake": "^2.0.2",
    "@types/react-native-share": "^3.3.2",
    "@types/react-native-svg-charts": "^5.0.9",
    "@types/react-test-renderer": "^16.9.2",
    "@types/sbd": "^1.0.2",
    "babel-jest": "^24.9.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-react-native": "^4.0.1",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "husky": "^4.3.8",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^26.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "node": "^16.13.1",
    "patch-package": "^6.2.2",
    "prettier": "^2.2.1",
    "react-devtools": "^4.10.0",
    "react-native-clean-project": "^3.2.4",
    "react-native-cli": "^2.0.1",
    "react-native-obfuscating-transformer": "^1.0.0",
    "react-native-rename": "^2.4.1",
    "react-native-typescript-transformer": "^1.2.13",
    "react-native-version": "^4.0.0",
    "react-test-renderer": "16.13.1",
    "standard-version": "^9.3.0",
    "ts-jest": "^26.1.3",
    "typescript": "^3.8.3",
    "yarn": "^1.22.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.(json,jsx,jsx,ts,tsx)": [
      "npm run prettify",
      "npm run lint:fix",
      "npm run lint"
    ]
  }
}
